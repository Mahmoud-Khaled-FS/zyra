POST {{BASE_URL}}/post

[headers]
Authorization = Bearer {{TOKEN}}

[query]
foo = bar
page = 1

[body]
{
  "json": "test"
}

[assert]
status eq 200

headers has "Content-Type"
headers.Content-Type startWith "application/json"
headers.Content-Type endWith "utf-8"

body is json
body.args is object

body.args.foo is string
body.args.foo eq "bar"

body.args.page is string
body.args.page eq "2"