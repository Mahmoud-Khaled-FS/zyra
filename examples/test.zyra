POST {{BASE_URL}}/post

[headers]
Authorization = Bearer {{TOKEN}}

[query]
foo = bar
page = 1

[body]
{
  "json": "test"
}

[assert]
res.status == 200

res.headers has "Content-Type"
res.headers.Content-Type == "application/json; charset=utf-8"

res.body is json
res.body.args is object

res.body.args.foo is string
res.body.args.foo == "bar"

res.body.args.page is string
res.body.args.page == "2"